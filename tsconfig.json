{
  "compilerOptions": {
    "sourceMap": true,
    "lib": ["es7", "dom", "esnext"],
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "target": "es5",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "esModuleInterop": true,
    "moduleResolution": "node"
  }
}
